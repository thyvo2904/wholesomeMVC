<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <add name="constr2" connectionString="data source=localhost\SQLEXPRESS;initial catalog=wholesomeDB;Trusted_Connection=Yes" providerName="System.Data.SqlClient"/>
    <add name="DefaultConnection" connectionString="data source=localhost\SQLEXPRESS;initial catalog=wholesomeDB;Trusted_Connection=Yes" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <authentication mode="Forms">
      <forms loginUrl="/Account/Registration" name=".ASPNETAUTH" protection="None" path="/" timeout="20"/>
    </authentication>
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"/>
    </httpModules>
    <compilation targetFramework="4.6" debug="true"/>
  </system.web>
  <system.web>
    <!--<roleManager enabled = "true"></roleManager>-->
  </system.web>
  <!-- Authentication required for these pages below -->
  <location path="Views/Account/Login">
    <system.web>
      <authorization>
        <allow roles="*"/>
      </authorization>
    </system.web>
  </location>
  <!--<location path="WebForms/index.aspx">
    <system.web>
      <authorization>
        <allow roles=""/>
      </authorization>
    </system.web>
  </location>-->
  <location path="~/WebForms/Update_Item.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
  <location path="WebForms/Sync_Database.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
  <!--<location path="WebForms/Populate_Table.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin" />
        <deny users="?" />
      </authorization>
    </system.web>
  </location>-->
